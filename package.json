{
  "name": "nodejs-typescript-template",
  "version": "0.0.0",
  "description": "A template for my nodejs projects",
  "main": "prod/index.js",
  "scripts": {
    "clean": "ts-node -r dotenv/config src/clean.ts",
    "continue": "ts-node -r dotenv/config src/continue.ts",
    "emails": "ts-node -r dotenv/config src/emails.ts",
    "init": "ts-node -r dotenv/config src/init.ts",
    "setup": "ts-node -r dotenv/config src/setup.ts",
    "teardown": "ts-node -r dotenv/config src/teardown.ts",
    "build": "tsc",
    "lint": "eslint src test --max-warnings 0",
    "test": "ts-mocha -u tdd  test/**/*.spec.ts --recursive --exit --timeout 10000"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/naomi-lgbt/nodejs-typescript-template.git"
  },
  "keywords": [
    "template",
    "typescript",
    "eslint",
    "nodejs",
    "prettier"
  ],
  "author": "Naomi Carrigan",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/naomi-lgbt/nodejs-typescript-template/issues"
  },
  "homepage": "https://github.com/naomi-lgbt/nodejs-typescript-template#readme",
  "dependencies": {
    "dotenv": "16.3.1",
    "ts-node": "10.9.1",
    "winston": "3.10.0"
  },
  "devDependencies": {
    "@nhcarrigan/eslint-config": "1.1.3",
    "@nhcarrigan/prettier-config": "1.0.1",
    "@nhcarrigan/typescript-config": "1.0.1",
    "@types/chai": "4.3.5",
    "@types/mocha": "10.0.1",
    "@types/node": "18.17.9",
    "chai": "4.3.8",
    "eslint": "8.47.0",
    "mocha": "10.2.0",
    "prettier": "2.8.8",
    "ts-mocha": "10.0.0",
    "typescript": "5.2.2"
  }
}